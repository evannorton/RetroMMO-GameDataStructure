{
    "compilerOptions": {
      "incremental": false,
      "target": "ES2015",
      "module": "commonjs",
      "outDir": "lib",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "useUnknownInCatchVariables": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "baseUrl": "./",
      "paths": {
        "retrommo-types/*": [
          "../*"
        ]
      }
    },
    "include": [
      "./**/*.ts"
    ],
    "exclude": []
  }